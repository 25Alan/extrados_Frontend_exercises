doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")

    block css-linkContent
    link(rel="icon" href="images/favicon.ico")
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w==" crossorigin="anonymous" referrerpolicy="no-referrer")
    
    title=titlePage
  body 
    header
      .containerLogo
        img(src="images/logoGoogle.svg" alt="logoGoogle" id="logoQuery_Log")
      nav.containerNav
        ul
          li 
            a(href=urlHome) Home
          li 
            a(href=urlInquiries) Inquiries
          li 
            a(href=urlContact) Contact Us
          li
            a(href=urlCalc, target="_blank") Calc Bootstrap
          li
            a(href=urlPockemon target="_blank") Pockemon
          li#checkDarkWhite
            label 
              input(type="checkbox" name="check")
              span
              i.fa-regular.fa-moon
    main 
      block content